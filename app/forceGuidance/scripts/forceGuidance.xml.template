<application>
<name>forceGuidance Application </name>

<dependencies>	
<port>/ikart/control:i</port>
<port>/ftObs/left_arm/wrench:o</port>
<port>/ftObs/right_arm/wrench:o</port>
</dependencies>

<module>
   <name>forceGuidance</name>
   <parameters></parameters>
   <node>ikart</node>
   <stdio>ikart</stdio>
   <tag>forceGuidance</tag>
</module>

<connection>
  <from>/ftObs/left_arm/wrench:o</from>
  <to>/forceGuidance/l_wrenches:i</to>
  <protocol>udp</protocol>
</connection>

<connection>
  <from>/ftObs/right_arm/wrench:o</from>
  <to>/forceGuidance/r_wrenches:i</to>
  <protocol>udp</protocol>
</connection>

<connection>
  <from>/forceGuidance/commands:o</from>
  <to>/ikart/control:i</to>
  <protocol>udp</protocol>
</connection>

</application>
